[32m+[m[32m# dir() â†’ Shows all attributes and methods (including special methods)[m
[32m+[m
[32m+[m[32mimport os       #Access the system level command[m
[32m+[m
[32m+[m[32m# Clear command for Windows ('cls') and Unix/Linux/Mac ('clear')[m
[32m+[m[32mos.system("clear")      #This method is used to run the system command using system method.[m
[32m+[m[32m# os.system("dir")        #This method is used to run the system command using system method.[m
[32m+[m
[32m+[m[32m# print("Str")[m
[32m+[m[32m# print(dir(str))      # For str class[m
[32m+[m[32mprint("OS related")[m
[32m+[m[32mprint(dir(os))[m
[32m+[m[32mdetails =  os.times()[m
[32m+[m[32mprint(details.system )[m
[32m+[m[32mprint("\n")[m
[32m+[m[32mprint("list")[m
[32m+[m[32mprint(dir(list))     # For list class[m
[32m+[m[32mprint("\n")[m
[32m+[m[32mprint("Dict")[m
